{
  "name": "@skyscanner/backpack-foundation",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": "^12.13.0",
    "npm": "^6.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skyscanner/backpack-foundation.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "npm run build:tokens && npm run build:svgs",
    "build:svgs": "lerna run svgs --scope bpk-svgs",
    "build:tokens": "lerna run tokens",
    "clean:lerna": "lerna clean --yes",
    "publish": "lerna bootstrap && lerna run tokens && lerna publish"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-chmod": "^3.0.0",
    "gulp-clone": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-iconfont": "^9.2.0",
    "gulp-jsonlint": "^1.2.2",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-theo": "^2.0.0",
    "husky": "^4.0.2",
    "lerna": "^3.15.0",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "merge-stream": "^2.0.0",
    "merge2": "^1.2.3",
    "react-svg-loader": "^3.0.1",
    "theo": "^8.1.3",
    "tinycolor2": "^1.4.1"
  }
}
